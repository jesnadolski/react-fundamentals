le</button>\r\n   </div>\r\n  </div>\r\n );\r\n};\r\n\r\nconst useNumHook = (num) => {\r\n const [queryNum, setQueryNum] = useState(num);\r\n const [results, setResults] = useState('');\r\n\r\n useEffect(() => {\r\n  if(queryNum !== ''){\r\n   fetch(`http://numbersapi.com/${queryNum}`)\r\n    .then(res => res.text())\r\n    .then(json => {\r\n     console.log(json);\r\n     setResults(json);\r\n    });\r\n  };\r\n }, [queryNum]);\r\n\r\n return [results, queryNum, setQueryNum];\r\n};\r\n\r\nconst useClicks = (initCount) => {\r\n const [clicks, setClicks] = useState(initCount);\r\n\r\n useEffect(() => {\r\n  document.title = `You have clicked ${clicks} times`;\r\n }, [clicks])\r\n return [clicks, setClicks]\r\n}\r\n\r\nexport default Hooks2;","C:\\Users\\12192\\Desktop\\BlueBadge\\react_fundamentals\\src\\components\\Effects.js",[],"C:\\Users\\12192\\Desktop\\BlueBadge\\react_fundamentals\\src\\components\\apps\\timer-apps\\TimePiecesApp.js",[],"C:\\Users\\12192\\Desktop\\BlueBadge\\react_fundamentals\\src\\components\\apps\\timer-apps\\TimerApp.js",[],"C:\\Users\\12192\\Desktop\\BlueBadge\\react_fundamentals\\src\\components\\apps\\timer-apps\\ClockApp.js",[],"C:\\Users\\12192\\Desktop\\BlueBadge\\react_fundamentals\\src\\components\\apps\\timer-apps\\StopWatchApp.js",[],"C:\\Users\\12192\\Desktop\\BlueBadge\\react_fundamentals\\src\\components\\apps\\nyt-app\\NytApp.js",[],"C:\\Users\\12192\\Desktop\\BlueBadge\\react_fundamentals\\src\\components\\apps\\nyt-app\\NytResults.js",[],{"ruleId":"93","replacedBy":"94"},{"ruleId":"95","replacedBy":"96"},{"ruleId":"97","severity":1,"message":"98","line":8,"column":8,"nodeType":"99","messageId":"100","endLine":8,"endColumn":31},{"ruleId":"97","severity":1,"message":"101","line":9,"column":21,"nodeType":"99","messageId":"100","endLine":9,"endColumn":31},{"ruleId":"97","severity":1,"message":"102","line":10,"column":19,"nodeType":"99","messageId":"100","endLine":10,"endColumn":27},{"ruleId":"97","severity":1,"message":"103","